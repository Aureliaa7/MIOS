@model MusicalInstrumentsShop.BusinessLogic.DTOs.LoginDto

@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row add-margin-top-190">
    <div class="col-md-4">
        <div class="card">
            <div class="card-title"><h5>Register result</h5></div>
            <div class="card-body text-danger">
               @foreach(var errorMessage in ViewBag.RegisterResult)
               {
                   <p>@errorMessage</p>
               }
            </div>
            <div>
                <a asp-action="Register" asp-controller="Account">Try again</a>
            </div>
        </div>
    </div>
</div>
